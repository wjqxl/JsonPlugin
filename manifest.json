{
  "name": "JsonPlugin",
  "icons": {
    "16": "icon/jsonview16.png",
    "48": "icon/jsonview48.png",
    "128": "icon/jsonview128.png"
  },
  "version": "1.0.0",
  "description": "Validate and view JSON documents",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "file:///*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
